    "@tsconfig/svelte": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.25.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "sass": "^1.32.12",
    "stylelint": "^13.13.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^22.0.0",
    "svelte": "^3.37.0",
