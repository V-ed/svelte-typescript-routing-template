{
  "name": "svelte-typescript-routing-template",
  "version": "0.0.1",
  "description": "",
  "author": "Guillaume Marcoux",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all -p generate:watch dev:no-gql",
    "dev:no-gql": "npm-run-all -p dev:routify dev:vite",
    "dev:routify": "routify",
    "dev:vite": "vite --host",
    "build": "npm-run-all -s build:routify build:vite",
    "build:routify": "routify -b",
    "build:vite": "vite build",
    "serve:vite": "vite preview",
    "validate": "npm-run-all validate:*",
    "validate:svelte": "svelte-check --diagnostic-sources \"js,svelte\"",
    "validate:ts": "tsc --noEmit",
    "preview": "npx -p sirv-cli sirv dist -s -p 5004",
    "lint": "npm-run-all lint:*",
    "lint:src": "eslint \"src/**/*.{t,j}s\"",
    "lint:css": "stylelint \"**/*.{css,svelte,html}\"",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:watch": "graphql-codegen --config codegen.yml --watch"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typed-document-node": "^1.18.8",
    "@graphql-codegen/typescript": "1.22.3",
    "@graphql-codegen/typescript-operations": "^1.18.2",
    "@mdi/js": "5.9.55",
    "@roxi/routify": "2.18.0",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.11",
    "@tsconfig/svelte": "2.0.1",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "eslint": "7.29.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.5",
    "sass": "1.35.1",
    "stylelint": "13.13.1",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-standard": "22.0.0",
    "svelte": "3.38.3",
    "svelte-check": "2.2.0",
    "svelte-materialify": "0.3.11",
    "svelte-preprocess": "4.7.3",
    "tslib": "2.3.0",
    "typescript": "4.3.4",
    "urql": "^2.0.3",
    "vite": "2.3.8",
    "vite-plugin-pwa": "0.8.1",
    "vite-plugin-windicss": "1.1.0",
    "vite-tsconfig-paths": "3.3.13",
    "windicss": "3.1.3"
  },
  "dependencies": {
    "@urql/svelte": "^1.2.2",
    "axios": "0.21.1",
    "graphql": "^15.5.1",
    "minimum-delayer": "0.2.1",
    "socket.io-client": "4.1.2",
    "subscriptions-transport-ws": "^0.9.19"
  },
  "routify": {
    "routifyDir": ".routify"
  }
}
