{
  "name": "svelte-typescript-routing-template",
  "version": "0.0.0",
  "description": "",
  "author": "Guillaume Marcoux",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "npm-run-all -p generate:watch dev:no-gql",
    "dev:no-gql": "npm-run-all -p dev:routify dev:vite",
    "dev:routify": "routify",
    "dev:vite": "vite --host",
    "build": "npm-run-all -s build:routify build:vite",
    "build:routify": "routify -b",
    "build:vite": "vite build",
    "serve:vite": "vite preview",
    "validate": "npm-run-all validate:*",
    "validate:svelte": "svelte-check --diagnostic-sources \"js,svelte\"",
    "validate:ts": "tsc --noEmit",
    "preview": "npx -p sirv-cli sirv dist -s -p 5004",
    "lint": "npm-run-all lint:*",
    "lint:src": "eslint \"src/**/*.{t,j}s\"",
    "lint:css": "stylelint \"**/*.{css,svelte,html}\"",
    "generate": "graphql-codegen -r dotenv/config --config codegen.yml dotenv_config_path=.env.local",
    "generate:watch": "graphql-codegen -r dotenv/config --config codegen.yml dotenv_config_path=.env.local --watch",
    "preversion": "npm run lint",
    "postversion": "git push"
  },
  "dependencies": {
    "@urql/svelte": "1.3.2",
    "axios": "0.24.0",
    "graphql": "15.7.2",
    "graphql-ws": "5.5.5",
    "minimum-delayer": "0.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/typed-document-node": "2.2.1",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@mdi/js": "6.5.95",
    "@roxi/routify": "2.18.3",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.30",
    "@tsconfig/svelte": "2.0.1",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "dotenv": "10.0.0",
    "eslint": "8.3.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.11",
    "postcss-html": "1.2.0",
    "sass": "1.43.5",
    "stylelint": "14.1.0",
    "stylelint-config-css-modules": "2.3.0",
    "stylelint-config-html": "1.0.0",
    "stylelint-config-standard": "24.0.0",
    "svelte": "3.44.2",
    "svelte-check": "2.2.10",
    "svelte-materialify": "0.3.11",
    "svelte-preprocess": "4.9.8",
    "svelte-windicss-preprocess": "4.1.7",
    "tslib": "2.3.1",
    "typescript": "4.5.2",
    "urql": "2.0.5",
    "vite": "2.6.14",
    "vite-plugin-html": "2.1.1",
    "vite-plugin-pwa": "0.11.8",
    "vite-plugin-windicss": "1.5.3",
    "vite-tsconfig-paths": "3.3.17",
    "windicss": "3.2.1"
  },
  "routify": {
    "routifyDir": ".routify"
  }
}
