{
  "name": "svelte-typescript-routing-template",
  "version": "0.0.1",
  "description": "",
  "author": "Guillaume Marcoux",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "routify -c dev:vite",
    "build": "routify -b && vite build",
    "dev:vite": "vite --host",
    "serve:vite": "vite preview",
    "build:vite": "vite build",
    "validate": "npm-run-all validate:*",
    "validate:svelte": "svelte-check --diagnostic-sources \"js,svelte\"",
    "validate:ts": "tsc --noEmit",
    "preview": "npx -p sirv-cli sirv dist -s -p 5004",
    "lint": "npm-run-all lint:*",
    "lint:src": "eslint \"src/**/*.{t,j}s\"",
    "lint:css": "stylelint \"**/*.{css,svelte,html}\""
  },
  "devDependencies": {
    "@mdi/js": "5.9.55",
    "@roxi/routify": "2.18.0",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.11",
    "@tsconfig/svelte": "2.0.1",
    "@typescript-eslint/eslint-plugin": "4.27.0",
    "@typescript-eslint/parser": "4.27.0",
    "eslint": "7.29.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.5",
    "sass": "1.35.1",
    "stylelint": "13.13.1",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-standard": "22.0.0",
    "svelte": "3.38.2",
    "svelte-check": "2.1.0",
    "svelte-materialify": "0.3.11",
    "svelte-preprocess": "4.7.3",
    "tslib": "2.3.0",
    "typescript": "4.3.4",
    "vite": "2.3.8",
    "vite-plugin-pwa": "0.8.1",
    "vite-plugin-windicss": "1.0.4",
    "vite-tsconfig-paths": "3.3.13",
    "windicss": "3.1.3"
  },
  "dependencies": {
    "axios": "0.21.1",
    "minimum-delayer": "0.2.1",
    "socket.io-client": "4.1.2"
  },
  "routify": {
    "routifyDir": ".routify"
  }
}
