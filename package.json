    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "csstype": "^3.0.7",
    "eslint": "^7.21.0",
    "snowpack": "^3.0.13",
    "svelte": "^3.35.0",
    "sveltestrap": "^3.13.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "minimum-delayer": "^0.1.0",
